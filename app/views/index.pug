doctype html
head
	meta(charset='utf-8')
	meta(name='viewport', content='initial-scale=1, maximum-scale=1, user-scalable=no, width=device-width' )
	title vidaTres - step
	include templates/head
body
	section.plan-cliente
		.container
			.row
				.col-xs-1
					figure.logotipo
						img(src="./img/logotipo-vidatres.png", alt="")
				.col-xs-9
					.step
						.step__linea
						.step__paso
							span.step__numero 1
						.step__paso.step__paso--active
							span.step__numero 2
						.step__paso
							span.step__numero 3
						.step__paso
							span.step__numero 4
				.col-xs-10
					.titulo
						span.titulo__icon.icon-avion
						h2.titulo__name paso 2
							span.titulo__descripcion selecciona clinicas y coberturas
						
						.auxiliares
							span.auxiliares__icon.icon-info
							span.auxiliares__icon.icon-audio
			.row
				.col-xs-5
					article.red
						.red__container
							.red__header
								h3.red__titulo RED Clásica
							.red__body
								figure.red__clinica
									img(src="./img/clinica-davila.jpg", alt="Logotipo clinica")
								figure.red__clinica
									img(src="./img/logo_clinica_vespucio.jpg", alt="Logotipo clinica")
								figure.red__clinica
									img(src="./img/Clinica Bcentenario.jpg", alt="Logotipo clinica")
							.red__footer
								.row
									.col-xs-5.col-xs-offset-1
										figure.red__clinica.red__clinica--vi
											img(src="./img/vidaintegra1.png", alt="Logotipo clinica")
									.col-xs-5
										figure.red__clinica
											img(src="./img/integramedica.png", alt="Logotipo clinica")
				.col-xs-5
					article.red.red--active
						.red__container
							.red__header
								h3.red__titulo RED Plus
							.red__body
								figure.red__clinica
									img(src="./img/Clinica Indisa.jpg", alt="Logotipo clinica")
								figure.red__clinica
									img(src="./img/Marca-UCCHRISTUS-01.jpg", alt="Logotipo clinica")
								figure.red__clinica
									img(src="./img/clinicatabancura.jpg", alt="Logotipo clinica")
							.red__footer
								.row
									.col-xs-5.col-xs-offset-1
										figure.red__clinica.red__clinica--vi
											img(src="./img/vidaintegra1.png", alt="Logotipo clinica")
									.col-xs-5
										figure.red__clinica
											img(src="./img/integramedica.png", alt="Logotipo clinica")
				.col-xs-5
					article.red
						.red__container
							.red__header
								h3.red__titulo RED Premium
							.red__body
								figure.red__clinica
									img(src="./img/u-los-andes.jpg", alt="Logotipo clinica")
								figure.red__clinica
									img(src="./img/clinica uc.jpg", alt="Logotipo clinica")
								figure.red__clinica
									img(src="./img/Clinica Santa Maria.jpg", alt="Logotipo clinica")
							.red__footer
								.row
									.col-xs-5.col-xs-offset-6
										figure.red__clinica
											img(src="./img/integramedica.png", alt="Logotipo clinica")
				.col-xs-5
					article.red
						.red__container
							.red__header
								h3.red__titulo RED Pro
							.red__body
								figure.red__clinica
									img(src="./img/logo-clinica-alemana-jpg.jpg", alt="Logotipo clinica")
								figure.red__clinica
									img(src="./img/Clinica Santa Maria.jpg", alt="Logotipo clinica")
							.red__footer
								.row
									.col-xs-5.col-xs-offset-6
										figure.red__clinica
											img(src="./img/integramedica.png", alt="Logotipo clinica")
												
		.plan
			.plan__header
				.plan__info
					p.plan__nombre-plan Mi Plan C-01
					p.plan__cliente Camila 
						span Aravena
					p.plan__saldo $1.250.000
					p.plan__locacion Región Metropolitana
					p.plan__edad 45 años
				.plan__action
					span.plan__edit.icon-edit
					.plan__beneficiarios
						span.plan__beneficiarios-icon.icon-hombre
						span.plan__beneficiarios-icon.icon-mujer
						a.plan__beneficiarios-link(href="javascript:;") 5 Beneficiarios
						
			.plan__body
				.plan__prestadores
					p.plan__prestadores-titulo Prestadores
					.plan__add
						p.plan__red Red Premium
						p.plan__red Red Plus
					span.plan__close.icon-cerrar
						
				.plan__cobertura
					.plan__chart
						p.plan__chart-titulo Cobertura
							span ambulatoria
						.plan__chart-detalle
							// aqui va el gráfico
						span.plan__close.icon-cerrar
					.plan__chart
						p.plan__chart-titulo Cobertura
							span hospilataria
						.plan__chart-detalle
							// aqui va el gráfico
						span.plan__close.icon-cerrar
						
				.plan__productos
					.plan__seguro
						p.plan__productos-titulo Seguro copago
						span.plan__uf 30 uf
						span.plan__close.icon-cerrar
					.plan__adicionales
						p.plan__productos-titulo Productos adicionales
						.plan__adicionales-productos
							.plan__icon.icon-asistencia
							.plan__icon.icon-dental
							.plan__icon.icon-farmacia
						span.plan__edit.icon-edit
							
				ul.plan__valores
					li.plan__valores-item
						p.plan__valor-nombre
							span.icon-Combined-Shape
							| Valor Plan
						p.plan__monto
							| 3,5 uf/ mes
							span.icon-info
					li.plan__valores-item
						p.plan__valor-nombre
							span.icon-Combined-Shape
							| Valor Copago
						p.plan__monto
							| 0,5 uf/ mes
							span.icon-info
					li.plan__valores-item
						p.plan__valor-nombre
							span.icon-Combined-Shape
							| Valor Ges
						p.plan__monto
							| 0,3 uf/ mes
							span.icon-info
					li.plan__valores-item
						p.plan__valor-nombre
							span.icon-Combined-Shape
							| Valor Productos adicionales
						p.plan__monto
							| 0,0 uf/ mes
							span.icon-info
								
			.plan__footer
				p.plan__total-titulo Valor total
				p.plan__total-saldo $160.000
					span 5 uf/mes
						
				.bar
					.bar__progreso
						p.bar__progreso-info bar__actual $130.000 - 4,5 uf
						p.bar__progreso-info bar__adicional $30.000 - 0,5 uf
					.bar__info
						p.bar__titulo Tu 7%
						p.bar__titulo Adicional
						
				span.plan__help.icon-audio
				a.plan__link(href="javascript:;") ver mi plan
				
				
							
					
				
												
			
	include templates/footer

	
	
